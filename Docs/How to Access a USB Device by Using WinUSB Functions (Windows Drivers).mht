<span style=3D"color:Blue;">int</span> _tmain(<span style=3D"color:Blue;=
">int</span> argc, _TCHAR* argv[])
{

    GUID guidDeviceInterface =3D OSR_DEVICE_INTERFACE; <span style=3D"co=
lor:Green;">//in the INF file</span>

    BOOL bResult =3D TRUE;

    PIPE_ID PipeID;

    HANDLE hDeviceHandle =3D INVALID_HANDLE_VALUE;
    WINUSB_INTERFACE_HANDLE hWinUSBHandle =3D INVALID_HANDLE_VALUE;
    =

    UCHAR DeviceSpeed;
    ULONG cbSize =3D 0;

    bResult =3D GetDeviceHandle(guidDeviceInterface, &amp;hDeviceHandle)=
;
    <span style=3D"color:Blue;">if</span>(!bResult)
    {
        <span style=3D"color:Blue;">goto</span> done;
    }

    bResult =3D GetWinUSBHandle(hDeviceHandle, &amp;hWinUSBHandle);
    <span style=3D"color:Blue;">if</span>(!bResult)
    {
        <span style=3D"color:Blue;">goto</span> done;
    }

    bResult =3D GetUSBDeviceSpeed(hWinUSBHandle, &amp;DeviceSpeed);
    <span style=3D"color:Blue;">if</span>(!bResult)
    {
        <span style=3D"color:Blue;">goto</span> done;
    }

    bResult =3D QueryDeviceEndpoints(hWinUSBHandle, &amp;PipeID);
    <span style=3D"color:Blue;">if</span>(!bResult)
    {
        <span style=3D"color:Blue;">goto</span> done;
    }

    bResult =3D SendDatatoDefaultEndpoint(hWinUSBHandle);
    <span style=3D"color:Blue;">if</span>(!bResult)
    {
        <span style=3D"color:Blue;">goto</span> done;
    }

    bResult =3D WriteToBulkEndpoint(hWinUSBHandle, &amp;PipeID.PipeOutId=
, &amp;cbSize);
    <span style=3D"color:Blue;">if</span>(!bResult)
    {
        <span style=3D"color:Blue;">goto</span> done;
    }

    bResult =3D ReadFromBulkEndpoint(hWinUSBHandle, &amp;PipeID.PipeInId=
, cbSize);
    <span style=3D"color:Blue;">if</span>(!bResult)
    {
        <span style=3D"color:Blue;">goto</span> done;
    }


    system(<span style=3D"color:#A31515;">"PAUSE"</span>);

done:
    CloseHandle(hDeviceHandle);
    WinUsb_Free(hWinUSBHandle);

    <span style=3D"color:Blue;">return</span> 0;
}


