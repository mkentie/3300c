 title=3D"Copy to clipboard." href=3D"javascript:if (window.epx.codeSnip=
pet)window.epx.codeSnippet.copyCode('CodeSnippetContainerCode_3527e183-f=
f86-49a4-847f-2c3e61d9dd73');">Copy</a>
            </div>
        </div>
        <div id=3D"CodeSnippetContainerCode_3527e183-ff86-49a4-847f-2c3e=
61d9dd73" class=3D"codeSnippetContainerCode" dir=3D"ltr">
            <div style=3D"color:Black;"><pre>

BOOL WriteToBulkEndpoint(WINUSB_INTERFACE_HANDLE hDeviceHandle, UCHAR* p=
ID, ULONG* pcbWritten)
{
    <span style=3D"color:Blue;">if</span> (hDeviceHandle=3D=3DINVALID_HA=
NDLE_VALUE || !pID || !pcbWritten)
    {
        <span style=3D"color:Blue;">return</span> FALSE;
    }

    BOOL bResult =3D TRUE;

    UCHAR szBuffer[] =3D <span style=3D"color:#A31515;">"Hello World"</s=
pan>;
    ULONG cbSize =3D strlen(szBuffer);
    ULONG cbSent =3D 0;

    bResult =3D WinUsb_WritePipe(hDeviceHandle, *pID, szBuffer, cbSize, =
&amp;cbSent, 0);
    <span style=3D"color:Blue;">if</span>(!bResult)
    {
        <span style=3D"color:Blue;">goto</span> done;
    }

    printf(<span style=3D"color:#A31515;">"Wrote to pipe %d: %s \nActual=
 data transferred: %d.\n"</span>, *pID, szBuffer, cbSent);
    *pcbWritten =3D cbSent;


done:
    <span style=3D"color:Blue;">return</span> bResult;

