BOOL ReadFromBulkEndpoint(WINUSB_INTERFACE_HANDLE hDeviceHandle, UCHAR* =
pID, ULONG cbSize)
{
    <span style=3D"color:Blue;">if</span> (hDeviceHandle=3D=3DINVALID_HA=
NDLE_VALUE)
    {
        <span style=3D"color:Blue;">return</span> FALSE;
    }

    BOOL bResult =3D TRUE;

    UCHAR* szBuffer =3D (UCHAR*)LocalAlloc(LPTR, <span style=3D"color:Bl=
ue;">sizeof</span>(UCHAR)*cbSize);
    =

    ULONG cbRead =3D 0;

    bResult =3D WinUsb_ReadPipe(hDeviceHandle, *pID, szBuffer, cbSize, &=
amp;cbRead, 0);
    <span style=3D"color:Blue;">if</span>(!bResult)
    {
        <span style=3D"color:Blue;">goto</span> done;
    }

    printf(<span style=3D"color:#A31515;">"Read from pipe %d: %s \nActua=
